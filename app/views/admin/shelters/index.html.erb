<h1>Shelters</h1>

<h2>All Shelters:</h2>
<% @shelters.each do |shelter| %>
  <p>
    <%= shelter.name %>
  </p>
<% end %>

<h2>Shelters with Pending Applications:</h2>
<section id="shelters_with_pending_apps">
  <p>
    <% @app_shelters.each do |shelter| %>
      <%= shelter.name %>
    <% end %>
  </p>
</section>
